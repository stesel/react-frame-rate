{
  "name": "react-frame-rate",
  "version": "1.1.9",
  "description": "Create smooth animation in React components with ~60FPS.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "author": "Leonid Trofymchuk <stesel23@gmail.com>",
  "license": "MIT",
  "homepage": "https://stesel.github.io/react-frame-rate-demo",
  "repository": {
    "type": "git",
    "url": "https://github.com/stesel/react-frame-rate.git"
  },
  "bugs": {
    "url": "https://github.com/stesel/react-frame-rate/issues"
  },
  "keywords": [
    "requestAnimatioFrame",
    "react",
    "smooth",
    "animation"
  ],
  "scripts": {
    "start": "parcel ./src/__example__/index.html",
    "build": "rm -rf ./lib && tsc",
    "test": "jest",
    "lint": "tslint --project ./tsconfig.json",
    "coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "git add -A src",
    "postversion": "git push --set-upstream origin HEAD && git push --tags"
  },
  "dependencies": {
    "react": "16.10.2"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@types/enzyme": "3.10.3",
    "@types/jest": "24.0.18",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.2",
    "babel-loader": "8.0.6",
    "babel-preset-react-app": "9.0.2",
    "coveralls": "3.0.6",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "jest": "24.9.0",
    "parcel-bundler": "1.12.4",
    "react-dom": "16.10.2",
    "react-test-renderer": "16.10.2",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.1.0",
    "typescript": "3.6.3"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ],
    "coveragePathIgnorePatterns": [
      "test-preprocessor.js",
      "test-setup.js",
      "test-shim.js"
    ]
  }
}
